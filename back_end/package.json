{
  "name": "crossword-builder-api",
  "description": "Crossword Builder API",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Taylor Hummon",
    "email": "taylor@taylorhummon.com"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 20.15.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "yarn",
    "database": "other",
    "framework": "koa",
    "transports": [
      "rest"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "compiled/index.js",
    "compiled/**/*.d.ts",
    "compiled/**/*.shared.js"
  ],
  "main": "compiled/index",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf compiled/ && tsc",
    "start": "node compiled/",
    "test": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "bundle:client": "yarn run compile && npm pack --pack-destination ../public"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.29",
    "@feathersjs/authentication": "^5.0.29",
    "@feathersjs/authentication-client": "^5.0.29",
    "@feathersjs/configuration": "^5.0.29",
    "@feathersjs/errors": "^5.0.29",
    "@feathersjs/feathers": "^5.0.29",
    "@feathersjs/koa": "^5.0.29",
    "@feathersjs/schema": "^5.0.29",
    "@feathersjs/transport-commons": "^5.0.29",
    "@feathersjs/typebox": "^5.0.29",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@feathersjs/cli": "5.0.29",
    "@feathersjs/rest-client": "^5.0.29",
    "@types/assert": "^1.5.10",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.0.0",
    "assert": "^2.1.0",
    "axios": "^1.7.2",
    "cross-env": "^7.0.3",
    "mocha": "^10.6.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
